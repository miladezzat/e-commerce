<div class="row">
  <div class="col-md-8 col-md-offset-2">
    <h1>{{userName}}'s Profile</h1>
    <hr>
    <h2>My Orders</h2>
    {{#each orders}}
    <div class="panel panel-success">
      <div class="panel-heading text-center"> <strong class="h1"> Order #</strong></div>
      <div class="panel-body">
        <ul class="list-group">
          {{# each this.items }}
          <li class="list-group-item">
            <span class="badge">{{ this.price }}</span>
            {{ this.item.title }} | {{ this.qty }} Units
          </li>
          {{/each}}
        </ul>
      </div>
      <div class="panel-footer">
        <strong>Totlal Price: $ {{ this.cart.totalPrice }} </strong>
      </div>
    </div>
    {{/each}}
  </div>
</div>
